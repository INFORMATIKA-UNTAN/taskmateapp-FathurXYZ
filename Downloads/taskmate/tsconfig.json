{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.js",
    "app/(tabs)/_layout.jsx",
    "constants/Colors.js",
    "app/modal.jsx",
    "app/+not-found.jsx",
    "app/+html.jsx",
    "app/_layout.jsx",
    "app/(tabs)/two.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/_layout.jsx",
    "app/_layout.jsx",
    "expo-env.d.ts"
  ]
}
